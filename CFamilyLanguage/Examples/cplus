class Some {
public:
    int sum(a, b) {
        return a + b;
    }

};


class Something : public Some {
public:
    void test() {
        std::cout << ("Calling parent class method:");
        int sumResult = Some::sum(10, 5);
        std::cout << (sumResult);
    }

};

Something().test();