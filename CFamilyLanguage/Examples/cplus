int arr[6] = {1, 2, 3, 2, 3, 4};
std::cout << ("Printing arr = [1, 2, 3, 2, 3, 4]:");
std::cout << (arr);
std::cout << ("Printing arr[1]:");
std::cout << (arr[1]);
std::cout << ("Changing arr[1] to 20:");
arr[1] = 20;
std::cout << (arr[1]);
